{
    "run_domain_enum": true,
    "run_cloud_scan": true,
    "run_web_app_scan": true,
    "run_api_scan": true,
    "run_ai_scan": true,
    "generate_report": true,
    "domain_enum": {
        "wordlist": "wordlists/subdomains.txt",
        "threads": 50,
        "timeout": 5
    },
    "cloud_scan": {
        "check_aws": true,
        "check_azure": true,
        "check_gcp": true,
        "check_kubernetes": true
    },
    "web_app_scan": {
        "crawl_depth": 2,
        "max_pages": 100,
        "test_forms": true,
        "test_jwt": true
    },
    "api_scan": {
        "test_graphql": true,
        "test_rest": true,
        "max_endpoints": 50
    },
    "ai_scan": {
        "model_path": "models/",
        "confidence_threshold": 0.7
    },
    "reporting": {
        "formats": ["pdf", "html", "json"],
        "include_screenshots": true,
        "risk_scoring": true
    }
}